import java.util.*;
public class Bai2 
{
	static int S=0;
	public static void main(String []args){
	System.out.println("Please input number: ");
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	for(int i=1;i<=n;i++)
	{ 
            S=(S+i)^2; 
	}
	System.out.println("Sum: " ,+S);
    }
}
